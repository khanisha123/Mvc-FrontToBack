@using FrontToBack.ViewModels
@model ForgetPassword

@{
    Layout = null;
}
<h1>Forget Password</h1>


<div class="container">
    <div class="row">
        <form method="post">
            <input asp-for="User.Email" placeholder="enter Email Address" />
            <button type="submit" class="btn btn-primary">Send Email</button>
        </form>
    </div>
</div>
